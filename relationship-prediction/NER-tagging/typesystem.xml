                <featureDescription>
                                        
                    <name>voice</name>
                                        
                    <description>Verbs: active/passive</description>
                                        
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                                    
                </featureDescription>
                                
                <featureDescription>
                                        
                    <name>definiteness</name>
                                        
                    <description>Definite or indefinite</description>
                                        
                    <rangeTypeName>uima.cas.String</rangeTypeName>
                                    
                </featureDescription>
                                
                <featureDescription>
                                        
                    <name>value</name>
                                        
